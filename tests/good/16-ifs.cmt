fun test(x : int) : int
	if x == 1
		return 1
	else if x == 2
		return 2
	else if x == 3
		return 3
	else if x == 4
		return 4
	else if x == 5
		return 5
	else if x == 6
		return 6
	else if x == 7
		return 7
	else
		return 8

