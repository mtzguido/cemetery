fun left() : bits
	var a = < 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x01 :: 80 >
	var b = < 0xff 0xff 0xff 0xff 0xff 0xff 0xff 0xff 0xff 0xff :: 80 >
	var c = < 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x01 :: 80 >
	var d = < 0xff 0xff 0xff 0xff 0xff 0xff 0xff 0xff 0xff 0xff :: 80 >
	return a + b + c + d

fun right() : bits
	return < 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 :: 80 >
