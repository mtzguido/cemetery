include ../../../lib/sha1.cmt

fun left() : bits
	var a = < 0x68 0x6f 0x6c 0x61 :: 32 > -- "hola"
	a = a ||| a
	a = a ||| a
	a = a ||| a
	a = a ||| a
	a = a ||| a
	-- a contains "hola" * 32

	return sha1(a)

fun right() : bits
	return < 0x22 0xd9 0x39 0xef 0xab 0x1c 0xb1 0x1d 0xdc 0x74 0x30 0xdc 0x23 0x5a 0xa1 0x04 0xac 0x6d 0xac 0x1d :: 160 >
